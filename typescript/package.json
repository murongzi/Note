  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/config.js ./src/favicon.ico ./dist",
    "build": "npm rebuild node-sass && npm run clean && npm run copy && webpack --progress --bail --color --env dist -p --optimize-minimize",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "test": "npm run test-ts && npm run test-js",
    "test-js": "mocha --compilers js:babel-core/register ./src/**/_tests_/**/*Test.js",
    "test-ts": "mocha --compilers js:ts-node/register ./src/**/_tests_/**/*Test.ts",
    "serve:dev": "webpack-dev-server --open --env dev",
    "serve:dist": "webpack-dev-server --env dist -p --progress",
    "start": "npm run serve:dev"
  },
